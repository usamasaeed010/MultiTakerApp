<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.multitakerapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MultiTakerApp">
        <activity
            android:name=".Auth.UploadImageActivity"
            android:exported="false" />
        <activity
            android:name=".Auth.AllHostelActivity"
            android:exported="false" />
        <activity
            android:name=".Auth.AddHostelActivity"
            android:exported="false" />
        <activity
            android:name=".Auth.SelectLoginActivity"
            android:exported="false" />
        <activity
            android:name=".Auth.LoginMainActivity"
            android:exported="false" />
        <activity
            android:name=".SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:exported="false" />
    </application>

</manifest>